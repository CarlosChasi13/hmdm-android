<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <objectAnimator
            android:propertyName="translationZ"
            android:duration="@integer/mtrl_fab_translation_z_press_duration"
            android:valueTo="@dimen/mtrl_fab_translation_z_pressed"
            android:valueType="floatType"/>
    </item>

    <item android:state_enabled="false">
        <objectAnimator
            android:propertyName="translationZ"
            android:duration="@integer/mtrl_fab_translation_z_state_list_duration"
            android:valueTo="@dimen/mtrl_fab_translation_z_disabled"
            android:valueType="floatType"/>
    </item>

    <item>
        <objectAnimator
            android:propertyName="translationZ"
            android:duration="@integer/mtrl_fab_translation_z_state_list_duration"
            android:valueTo="@dimen/mtrl_fab_translation_z_default"
            android:valueType="floatType"/>
    </item>

</selector>
